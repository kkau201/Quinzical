<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="800.0" prefWidth="1280.0" stylesheets="@../Stylesheet.css" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="quinzical.other.MenuController">
   <ImageView fitHeight="800.0" fitWidth="1280.0" pickOnBounds="true" preserveRatio="true">
      <Image url="@../resources/img/Menu_BG.png" />
   </ImageView>
   <VBox alignment="CENTER" layoutX="698.0" layoutY="270.0" prefHeight="386.0" prefWidth="400.0">
      <children>
         <Button fx:id="play" lineSpacing="100.0" mnemonicParsing="false" onAction="#setPlay" prefHeight="80.0" prefWidth="400.0" style="-fx-pref-width: 350; -fx-pref-height: 60;" text="PLAY" textAlignment="CENTER">
            <VBox.margin>
               <Insets bottom="10.0" />
            </VBox.margin>
         </Button>
         <Button fx:id="practice" mnemonicParsing="false" onAction="#setPractice" prefHeight="80.0" prefWidth="400.0" style="-fx-pref-width: 350; -fx-pref-height: 60;" text="PRACTICE" textAlignment="CENTER">
            <VBox.margin>
               <Insets bottom="10.0" />
            </VBox.margin>
         </Button>
         <!--<Button fx:id="settings" mnemonicParsing="false" onAction="#setSettings" prefHeight="80.0" prefWidth="400.0" style="-fx-pref-width: 350; -fx-pref-height: 60;" text="SETTINGS" textAlignment="CENTER">
            <VBox.margin>
               <Insets bottom="10.0" />
            </VBox.margin>
         </Button>-->
         <Button id="quit" fx:id="quit" mnemonicParsing="false" onAction="#setQuit" prefHeight="80.0" prefWidth="400.0" style="-fx-pref-width: 350; -fx-pref-height: 60;" text="QUIT" textAlignment="CENTER" />
      </children>
   </VBox>
</AnchorPane>
